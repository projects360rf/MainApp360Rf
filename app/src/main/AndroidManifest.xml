<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.a360rfandroidapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.360RFAndroidApp">
        <activity android:name=".DetailsDepartmentActivity"></activity>
        <activity android:name=".DetailsInstitutionActivity"></activity>
        <activity android:name=".DetailsActivity" />
        <activity
            android:name=".mainNewsOurActivityPage"
            android:label="@string/title_activity_main_news_our_page"
            android:theme="@style/Theme.360RFAndroidApp.NoActionBar" />
        <activity
            android:name=".Splash"
            tools:ignore="ExtraText">
            android:label="360RF"
            android:theme="@style/Theme.Theme.360RFAndroidApp.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".mainAboutPage"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>